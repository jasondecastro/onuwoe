<!DOCTYPE html>
<html>
  <head>
    <title>Onuwoe</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      Signed in as <%= current_user.name %> | <%= link_to 'Sign Out', '/signout' %>
    <% else %>
      <%= link_to 'Sign In', root_path %> | <%= link_to 'Sign Up', '/signup' %>
    <% end %>
    <%= yield %>
  </body>
</html>
